<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service
                id="behat3_symfony_extension.subscriber.reboot_kernel"
                class="Yoanm\Behat3SymfonyExtension\Subscriber\RebootKernelSubscriber"
        >
            <argument type="service" id="behat3_symfony_extension.logger.sf_kernel_logger" />
            <tag name="event_dispatcher.subscriber"/>
        </service>
    </services>
</container>
