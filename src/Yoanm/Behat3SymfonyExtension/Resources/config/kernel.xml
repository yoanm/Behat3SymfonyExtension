<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="behat3_symfony_extension.kernel" class="%behat3_symfony_extension.kernel.class%">
            <argument>%behat3_symfony_extension.kernel.env%</argument>
            <argument>%behat3_symfony_extension.kernel.debug%</argument>
            <file>%paths.base%/%behat3_symfony_extension.kernel.path%</file>
        </service>
    </services>
</container>
